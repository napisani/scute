{
	"prompt": "scute-test$ ",
	"steps": [
		{ "send_line": "export PS1=\"scute-test$ \"" },
		{ "wait_for_prompt": true },
		{ "send_line": "eval \"$({scute_bin} --config {config} init {shell})\"" },
		{ "wait_for_prompt": true },
		{ "clear_buffer": true },
		{
			"send_line": "{scute_bin} --config {config} choose \"ls -la /tmp\" 5"
		},
		{ "wait_for_text": "Scute", "timeout": 5 },
		{ "send_keys": ["DOWN"] },
		{ "sleep": 0.1 },
		{ "send_keys": ["DOWN"] },
		{ "sleep": 0.1 },
		{ "send_keys": ["DOWN"] },
		{ "sleep": 0.1 },
		{ "send_keys": ["UP"] },
		{ "sleep": 0.1 },
		{ "send_keys": ["UP"] },
		{ "sleep": 0.1 },
		{ "send_keys": ["UP"] },
		{ "sleep": 0.1 },
		{ "send_keys": ["ENTER"] },
		{ "wait_for_prompt": true, "timeout": 15 },
		{ "assert_output_contains": "ls" },
		{ "send_line": "exit" }
	]
}
